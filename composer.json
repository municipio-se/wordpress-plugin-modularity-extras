{
  "name": "municipio/wordpress-plugin-modularity-extras",
  "description": "Adds more Modularity modules",
  "type": "wordpress-plugin",
  "license": "AGPL-3.0-or-later",
  "authors": [
    {
      "name": "Whitespace Dev",
      "email": "code@whitespace.se"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "ModularityExtras\\": "src/"
    }
  },
  "require": {
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "helsingborg-stad/modularity": "^2.13",
    "wp-graphql/wp-graphql-acf": "^0.5.1",
    "wp-graphql/wp-graphql": "^1.3",
    "wpackagist-plugin/advanced-custom-fields": "^5.9"
  },
  "extra": {
    "installer-name": "modularity-extras"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
